{
  "dependencies": {
    "@creativebulma/bulma-badge": "1.0.1",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@nuxtjs/apollo": "4.0.1-rc.3",
    "@nuxtjs/auth": "4.9.1",
    "@nuxtjs/fontawesome": "1.1.2",
    "@sanity/client": "1.149.18",
    "@sanity/image-url": "0.140.19",
    "date-fns": "2.15.0",
    "graphql-tag": "2.11.0",
    "groq": "1.149.16",
    "nuxt": "2.14.1",
    "nuxt-buefy": "0.3.31",
    "nuxt-i18n": "6.13.8",
    "nuxt-stack": "0.8.1",
    "sanity-blocks-vue-component": "0.1.0",
    "vuelidate": "0.7.5"
  },
  "scripts": {
    "dev": "nuxt",
    "clean": "nuxt clean",
    "serve": "nuxt serve",
    "start": "nuxt start",
    "stats": "nuxt stats",
    "build": "nuxt build --modern=server",
    "generate": "nuxt generate --modern=client",
    "format": "nuxt format --write --lint '*.{md}' 'src/**/*.{js,json,scss,vue}'",
    "lint": "nuxt lint . --fix",
    "test": "nuxt test",
    "commit": "commit"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "nuxt format --write --lint",
      "nuxt lint --fix",
      "nuxt test --related",
      "git add"
    ],
    "src/**/*.{html,json,scss}": [
      "nuxt format --write",
      "git add"
    ],
    "*.{md}": [
      "nuxt format --write",
      "git add"
    ]
  },
  "devDependencies": {}
}
